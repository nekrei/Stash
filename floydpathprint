#include<bits/stdc++.h>
using namespace std;
typedef long long ll;

const ll INF = 1e15;
void solve(){
    int n,m;
    cin>>n>>m;
    vector<vector<ll>> am(n+1, vector<ll>(n+1,INF));
    vector<vector<ll>> s(n+1, vector<ll>(n+1,-1));
    for(int i=0; i<=n; i++){
        am[i][i] = 0;
        s[i][i] = i;
    }
    for(int i=0; i<m; i++){ 
        int a,b,w;
        cin>>a>>b>>w;
        am[a][b]=w;
        s[a][b]=b;
    }
    
    for(int k=1; k<=n; k++){
        for(int i=1; i<=n; i++){
            for(int j=1; j<=n; j++){
                if(am[i][j] > am[i][k] + am[k][j]){
                    am[i][j] = am[i][k] + am[k][j];
                    s[i][j] = s[i][k];
                }
            }
        }
    }
    // for(int i=1; i<=n; i++){
    //         for(int j=1; j<=n; j++){
    //             cout<<s[i][j]<<" ";
    //         }
    //         cout<<'\n';
    //     }

    int q;
    cin>>q;
    for(int i=0; i<q; i++){
        int a,b;
        cin>>a>>b;
        if(am[a][b]==INF) {
            cout<<"NO PATH\n";
            continue;
        }
        cout<<am[a][b]<<' ';
        for(int cur = a; cur != b; cur = s[cur][b]){
            cout<<cur<<' ';
        }
        cout<<b<<'\n';
    }

}

int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
        solve();
    return 0;
}
